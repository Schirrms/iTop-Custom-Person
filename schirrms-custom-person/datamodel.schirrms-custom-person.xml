<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
	<constants>
	</constants>
	<classes>
		<class id="Person" _delta="must_exist">
			<properties>
				<uniqueness_rules>
					<rule id="employee_number">
						<filter _delta="redefine"><![CDATA[employee_number != '' AND employee_number != '0000000']]></filter>
					</rule>
				</uniqueness_rules>
			</properties>
			<fields>
				<field id="loginname" xsi:type="AttributeString" _delta="define">
					<sql>loginname</sql>
					<default_value/>
					<is_null_allowed>true</is_null_allowed>
				</field>
			</fields>
			<presentation>
				<details>
					<items>
						<item id="col:col1" _delta="must_exist">
							<items>
								<item id="fieldset:Person:info" _delta="must_exist">
									<items>
										<item id="loginname" _delta="define">
											<rank>75</rank>
										</item>
									</items>
								</item>
							</items>
						</item>
					</items>
				</details>
				<search>
					<items>
						<item id="loginname" _delta="define">
							<rank>55</rank>
						</item>
					</items>
				</search>
			</presentation>
		</class>
	</classes>
	<menus>
	</menus>
	<user_rights>
		<groups>
		</groups>
		<profiles>
		</profiles>
	</user_rights>
</itop_design>
